{
  "name": "@ddd/slides",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "yon build:web && yon build:onepage && yon build:pdf",
    "build:web": "cross-env MDXP_MODE=web PUBLIC_PATH=./ ANALYZE=true webpack --mode production",
    "build:onepage": "cross-env MDXP_MODE=onepage webpack --mode production && rimraf dist/onepage/*.js dist/onepage/*.css",
    "clean": "rimraf dist/**/* dist/*.pdf",
    "clean:web": "rimraf dist/web/*",
    "clean:onepage": "rimraf dist/onepage/*"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions and > 0.1%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.10.4",
    "@mdx-js/loader": "^1.5.9",
    "@mdxp/pdf": "^0.2.1",
    "@mdxp/rehypex-plugins": "^0.2.1",
    "@svgr/webpack": "^5.4.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "mdast-flatten-image-paragraphs": "^1.0.0",
    "react-dev-utils": "^10.2.1",
    "rehype-katex": "^4.0.0",
    "remark-code-import": "^0.2.0",
    "remark-emoji": "^2.1.0",
    "remark-math": "^3.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.0",
    "webpack": "^5.57.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpackbar": "^4.0.0",
    "yarn-or-npm": "^3.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/react": "^1.5.9",
    "@mdxp/components": "^0.2.1",
    "@mdxp/core": "^0.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "theme-ui": "^0.13.1"
  },
  "version": "1.0.0"
}
